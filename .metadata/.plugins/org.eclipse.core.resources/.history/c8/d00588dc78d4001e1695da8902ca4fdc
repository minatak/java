package t01;

public class Test4 {
	// main메소드
	public static void main(String[] args) {
		// 콘솔에 내용 출력: sysout
		System.out.println("1.반갑\t\s습니다람쥐");	// \t는 탭, \은 엔터키(줄바꾸기)
		System.out.print("2.반갑습니다람쥐\n\n");
		System.out.println("3.반갑습니다람쥐");
		
		/*
		   이곳은 여러줄의 주석처리
		   여러줄에 설명을 기술할 수 있습니당.
		 */
		
		System.out.println("4.반갑습니다람쥐");
	}
}
